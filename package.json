{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run pre-dev && vite",
    "build": "npm run pre-build && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "pre-dev": "npm run typecheck && npm run lint",
    "pre-build": "npm run typecheck && npm run lint && npm run test:ui-functional",
    "verify-build": "tsx scripts/verify-build.ts",
    "verify-quick": "npm run typecheck && npm run lint",
    "test": "vitest",
    "test:ui-functional": "tsx scripts/ui-test-runner.ts",
    "test:ui-vitest": "vitest run src/test/ui-functional-tests.ts",
    "test:interactions": "tsx scripts/ui-test-runner.ts --focus interactions",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:unit": "vitest run src/__tests__/**/*.test.ts --exclude=**/e2e/**",
    "test:integration": "vitest run src/__tests__/integration/**/*.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test --grep='@smoke'",
    "test:auto": "tsx src/utils/testWatcher.ts",
    "test:generate": "tsx src/utils/autoTestGenerator.ts",
    "test:ai": "tsx src/agents/testing/TestRunner.ts --mode manual --agents user,admin --testLevel comprehensive",
    "test:imports": "node scripts/test-imports.js",
    "test:components": "vitest run src/components/task-board/__tests__",
    "test:data": "vitest run src/data/__tests__",
    "pre-commit": "node scripts/pre-commit-checks.js",
    "validate": "npm run test:imports && npm run test:components && npm run test:data",
    "test:ai:smoke": "tsx src/agents/testing/TestRunner.ts --mode dev --agents user --personas alex_neurodiverse --testLevel smoke",
    "test:ai:ci": "tsx src/agents/testing/TestRunner.ts --mode ci --agents user,admin --testLevel comprehensive --failFast true",
    "test:ai:accessibility": "tsx src/agents/testing/TestRunner.ts --mode manual --agents user --personas alex_neurodiverse --testLevel comprehensive",
    "test:ai:security": "tsx src/agents/testing/TestRunner.ts --mode manual --agents admin --testLevel extensive",
    "test:ai:performance": "tsx src/agents/testing/TestRunner.ts --mode manual --agents admin --testLevel extensive",
    "test:ai:user-journeys": "tsx src/agents/testing/TestRunner.ts --mode manual --agents user --personas alex_neurodiverse,sam_entrepreneur,morgan_methodical --testLevel comprehensive",
    "test:ai:report": "tsx src/agents/testing/TestRunner.ts --mode manual --agents user,admin --outputFormat html --reportPath ./test-reports/ai-test-report",
    "test:ai:integration": "vitest run src/__tests__/ai-testing-integration.test.ts",
    "test:full": "npm run test:unit && npm run test:ai:smoke && npm run test:integration",
    "test:enhanced": "tsx src/agents/testing/advanced/EnhancedTestRunner.ts --mode manual --testLevel comprehensive --outputFormat all",
    "test:enhanced:ci": "tsx src/agents/testing/advanced/EnhancedTestRunner.ts --mode ci --testLevel comprehensive --failFast true --outputFormat json",
    "test:enhanced:production": "tsx src/agents/testing/advanced/EnhancedTestRunner.ts --mode production --testLevel extensive --outputFormat all",
    "test:ai-quality": "tsx src/agents/testing/advanced/AIQualityTestAgent.ts",
    "test:visual-regression": "tsx src/agents/testing/advanced/VisualRegressionTestAgent.ts",
    "test:load-testing": "tsx src/agents/testing/advanced/LoadTestAgent.ts",
    "test:security": "tsx src/agents/testing/advanced/EnhancedTestRunner.ts --mode manual --include_security_testing true --testLevel comprehensive",
    "test:comprehensive": "npm run test:enhanced && npm run test:e2e && npm run test:unit",
    "test:performance": "npm run test:load-testing && npm run test:ai:performance",
    "test:quality-gate": "tsx src/agents/testing/advanced/EnhancedTestRunner.ts --mode production --failFast true --performance_thresholds.quality_score 85",
    "schema:check": "tsx src/lib/schema-validator-cli.ts",
    "schema:fix": "tsx src/lib/schema-validator-cli.ts --auto-fix",
    "schema:validate": "npm run schema:check && echo 'Schema validation complete'",
    "deploy:dev": "tsx scripts/deploy.ts development",
    "deploy:staging": "tsx scripts/deploy.ts staging",
    "deploy:prod": "tsx scripts/deploy.ts production",
    "typecheck": "tsc --noEmit",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "lucide-react": "^0.525.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.3",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "husky": "^8.0.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "chokidar": "^3.6.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
